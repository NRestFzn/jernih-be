{
	"compilerOptions": {
		"target": "es2016",
		"module": "commonjs",
		"baseUrl": "./src",
		// "paths": {
		// 	"@/*": ["*"]
		// },
		"moduleResolution": "Node",
		"outDir": "dist",
		"importsNotUsedAsValues": "remove",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"types": ["vitest/globals"],
		"resolveJsonModule": true,
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"]
}
